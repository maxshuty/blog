{
    "name": "frontend",
    "version": "1.0.0",
    "description": "A personal blog website",
    "author": "Max Poshusta",
    "private": true,
    "scripts": {
        "all": "npm-run-all --parallel dev backend",
        "dev": "nuxt",
        "backend": "cd ../backend && npm run dev",
        "build": "nuxt build",
        "start": "nuxt start",
        "generate": "nuxt generate",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
        "cleanup": "npm run lint -- --fix",
        "test": "jest",
        "productionBuild": "cross-env NODE_ENV=production npm run build",
        "productionRun": "cross-env NODE_ENV=production npm run start",
    },
    "dependencies": {
        "@nuxtjs/apollo": "^4.0.0-rc18",
        "@nuxtjs/markdownit": "^1.2.7",
        "cross-env": "^6.0.3",
        "graphql": "^14.5.8",
        "nuxt": "^2.0.0",
        "@nuxtjs/vuetify": "^1.0.0"
    },
    "devDependencies": {
        "@nuxtjs/eslint-config": "^1.0.1",
        "@nuxtjs/eslint-module": "^1.0.0",
        "@vue/test-utils": "^1.0.0-beta.27",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.1.0",
        "eslint": "^6.1.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-nuxt": ">=0.4.2",
        "eslint-plugin-prettier": "^3.0.1",
        "jest": "^24.1.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.16.4",
        "vue-jest": "^4.0.0-0"
    }
}
